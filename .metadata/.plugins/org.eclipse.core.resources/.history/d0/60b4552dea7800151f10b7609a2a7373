
public class Calc {

	int top;
	int[] stack;
	int max = 100;
	
	public Calc(){
		top = 0;
		stack = new int[max];
	}
	
	public void push(int key){
		stack[top] = key;
		top++;
		System.out.println(key);
	}
	
	public int pop(){
		return stack[top--];
	}
	
	public int peek(){
		return stack[top];
	}
	
	public void add(){
		stack[top-1] = stack[top] + stack[top-1];
		top--;
	}
	
	public void subtract(){
		stack[top-1] = stack[top]-stack[top-1];
		top--;
	}
	
	public void multiply(){
		stack[top-1] = stack[top] * stack[top-1];
		top--;
	}
	
	public void divide(){
		stack[top-1] = stack[top] / stack[top-1];
		top--;
	}
	
	public int depth(){
		return top+1;
	}
}
